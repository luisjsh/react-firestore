round-color: white;\n        color: #5D4E7B;\n    }\n`\n\n\nconst Square = styled.div`\n    width: 50px;\n    height: 50px;\n    border-radius: 8px;\n    background-repeat: no-repeat;\n    background-position: center;\n    background-size: 35%;\n    ${props =>{\n        switch(props.type){\n            case 'deposit':\n                return `\n                    background-color: #DDFABF;\n                    background-image: url(${DepositIcon});\n                `\n\n            case 'withdraw':\n                return `\n                \n                `\n            \n                default: \n                return ''\n        }\n    }}\n`\n\nfunction AddButton({type}) {\n    const history = useHistory()\n\n    const Redirect = (e)=>{\n        let {name} = e.target\n        history.push(`/add/${name}`)\n    }\n\n    if(type) return <Square type={type} />\n    \n\n    if(!type) return (\n        <ButtonWrapper>\n            <OptionsWrapper>\n                <Span name='transaction' onClick={Redirect}>Add Transactions</Span>\n                <Span name='bank-account' onClick={Redirect}>Add Bank account</Span>\n            </OptionsWrapper>\n            <Button></Button>\n        </ButtonWrapper>\n    )\n}\n\nexport default AddButton","/media/luisjsh/56DEB2D6DEB2AE1D/proyectos/finances/src/privateRoute.js",[],"/media/luisjsh/56DEB2D6DEB2AE1D/proyectos/finances/src/pages/add/transactions/add-transactions.jsx",[],"/media/luisjsh/56DEB2D6DEB2AE1D/proyectos/finances/src/pages/add/bank-account/add-bank-account.jsx",[],"/media/luisjsh/56DEB2D6DEB2AE1D/proyectos/finances/src/components/navbar/navbar.jsx",[],"/media/luisjsh/56DEB2D6DEB2AE1D/proyectos/finances/src/components/navbar/responsive/desktop.jsx",[],"/media/luisjsh/56DEB2D6DEB2AE1D/proyectos/finances/src/components/navbar/responsive/mobile.jsx",[],"/media/luisjsh/56DEB2D6DEB2AE1D/proyectos/finances/src/components/errortab.jsx",[],"/media/luisjsh/56DEB2D6DEB2AE1D/proyectos/finances/src/data-api/BankAccountCons